
<root BTCPP_format="4" main_tree_to_execute="SOLUTION_THREE">
  	<BehaviorTree ID="SOLUTION_THREE">
		<Repeat num_cycles="5">
		<NoObjectsNearby metric="{@safe_metric}" laserScan="{@laser_scan}" nearbyPercentage="0.30" out_status="{@nearby_status}">
		<AdaptMaxVelocity adaptation_period="3" adaptation_subject="max_velocity" subject_location="velocity_smoother" in_nearby_qr="{@nearby_status}">
				<Sequence>
				<roamTo pose="-6.1;4.1;0;0;0;0;0;" action_name="navigate_to_pose"/>
				<Sleep msec="10000"/>
				</Sequence>
		</AdaptMaxVelocity>
		</NoObjectsNearby>
		</Repeat>
  </BehaviorTree>
</root>
