
<root BTCPP_format="4" main_tree_to_execute="SOLUTIONAGAIN">
  	<BehaviorTree ID="SOLUTIONAGAIN">
		<Repeat num_cycles="5">
		<NoObjectsNearby metric="{@safe_metric}" laserScan="{@laser_scan}" nearbyPercentage="0.30" out_status="{@nearby_status}">
		<CPULimit metric="{@cpu_metric}" batteryState="{@battery_state}" cpuMessage="{@cpu_usage}" out_status="{@cpu_qr_status}">
		<AdaptPlanner adaptation_period="3" adaptation_subject="planner" subject_location="mirte_arborist_node" in_nearby_qr="{@nearby_status}">
		<RetryUntilSuccessful num_attempts="-1">
				
				<Sequence>
				<roamTo pose="-6.1;4.1;0;0;0;0;0;" action_name="navigate_to_pose"/>
				<Sleep msec="1000"/>
				</Sequence>
		</RetryUntilSuccessful>

		</AdaptPlanner>
		</CPULimit>
		</NoObjectsNearby>
		</Repeat>
  </BehaviorTree>
</root>
